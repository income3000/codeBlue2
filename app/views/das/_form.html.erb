<%= form_with(model: da) do |form| %>
  <% if da.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(da.errors.count, "error") %> prohibited this da from being saved:</h2>

      <ul>
        <% da.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :city, style: "display: block" %>
    <%= form.text_field :city %>
  </div>

  <div>
    <%= form.label :caseNumber, style: "display: block" %>
    <%= form.number_field :caseNumber %>
  </div>

  <div>
    <%= form.label :feedback, style: "display: block" %>
    <%= form.text_field :feedback %>
  </div>

  <div>
    <%= form.label :rating, style: "display: block" %>
    <%= form.text_field :rating %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
